@namespace Tavenem.Blazor.Framework

@inherits TavenemComponentBase

<tf-popover @ref="ElementReference"
            @attributes="AdditionalAttributes"
            id="@Id"
            class="@CssClass"
            style="@CssStyle"
            tabindex="0"
            anchor-id="@AnchorId"
            focus-id="@FocusId"
            offset-x="@OffsetX"
            offset-y="@OffsetY"
            position-x="@PositionX"
            position-y="@PositionY"
            @onfocuslost="FocusOut">
    @ChildContent
</tf-popover>